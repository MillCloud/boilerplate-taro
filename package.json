    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "~6.0.0",
    "is-ci": "~3.0.0",
    "lint-md-cli": "~0.1.2",
    "lint-staged": "~11.0.0",
    "markdownlint-cli": "~0.27.1",
    "prettier": "~2.3.0",
    "stylelint": "~13.13.1",
    "typescript": "~4.2.4"
