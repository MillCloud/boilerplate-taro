    "lint:script": "eslint . --ext .js,.jsx,.ts,.tsx,.vue --fix",
    "lint:style": "stylelint ./**/*.{css,less,sass,scss,vue} --fix",
    "lint:markdown": "markdownlint . --fix && lint-md . --fix",
    "lint:ls": "ls-lint .",
